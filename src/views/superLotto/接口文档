/ops/api/lotto/user-progress    用户进度
{
  "code": 0,
  "data": {
    "buyTime": 0,
    "gameProgress": {
      "gameBetting": 0,
      "progressList": [
        {
          "amount": 0,
          "awards": 0,
          "propType": 0,
          "sort": 0,
          "status": 0
        }
      ]
    },
    "taskProgress": [
      {
        "finishNum": 0,
        "idx": 0,
        "state": 0,
        "totalNum": 0
      }
    ]
  },
  "message": "string"
}
ResponseBean«LottoUserProgressRsp» {
code (integer, optional),
data (LottoUserProgressRsp, optional): 响应数据体 ,
message (string, optional): 错误信息
}LottoUserProgressRsp {
buyTime (integer, optional): 购买礼包次数 ,
gameProgress (LottoGameInfoRsp, optional): 游戏进度 ,
taskProgress (Array[SupremeActivityTaskProgressRsp], optional): 任务进度 idx=1每日任务 idx=2充值任务
}LottoGameInfoRsp {
gameBetting (integer, optional): 游戏累消 ,
progressList (Array[LottoGameProgressRsp], optional): 累消进度
}SupremeActivityTaskProgressRsp {
finishNum (integer, optional): 完成数 ,
idx (integer, optional): 位置 1=每日任务 2=充值任务 ,
state (integer, optional): 状态 0=未完成 1=已完成未领取 2=已完成已领取 ,
totalNum (integer, optional): 总数
}LottoGameProgressRsp {
amount (integer, optional): 流水 ,
awards (integer, optional): 奖励数 ,
propType (integer, optional): 类型 1=药丸 2=酒精 3=疫苗 ,
sort (integer, optional): 顺序 ,
status (integer, optional): 状态 0=未完成 1=待领取 2=已领取
}





var arr = [...(new Array(1000)).fill(0),...(new Array(1000)).fill(1),...(new Array(1000)).fill(2),...(new Array(1000)).fill(3),...(new Array(1000)).fill(4),...(new Array(1000)).fill(5),...(new Array(1000)).fill(6),...(new Array(1000)).fill(7),...(new Array(1000)).fill(8),...(new Array(1000)).fill(9)];
arr.sort(function(){
return Math.random() - 0.5;
})
arr.sort(function(){
return Math.random() - 0.5;
})
function arrTrans(num, arr) {
  const newArr = [];
  while(arr.length > 0) {
    newArr.push(arr.splice(0, num));
  }
  return newArr;
}
console.log(arrTrans(4,arr));