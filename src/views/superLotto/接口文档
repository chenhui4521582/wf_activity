/ops/api/lotto/activity-info     活动信息
{
  "code": 0,
  "data": {
    "awardPool": 0,
    "beginDate": "string",
    "countdown": 0,
    "endDate": "string",
    "numGroup": [
      {
        "id": 0,
        "numGroup": [
          0
        ]
      }
    ],
    "whatDay": 0,
    "state": 0,
    "timeline": "string",
    "userNumCount": 0
  },
  "message": "string"
}
ResponseBean«LottoActivityInfoRsp» {
code (integer, optional),
data (LottoActivityInfoRsp, optional): 响应数据体 ,
message (string, optional): 错误信息
}LottoActivityInfoRsp {
awardPool (integer, optional): 奖池 ,
beginDate (string, optional): 开始日期 ,
countdown (integer, optional): 当天的开奖倒计时 ,
endDate (string, optional): 结束日期 ,
numGroup (Array[UserNumGroupInfoRsp], optional): 第一页的用户号码组 ,
whatDay (integer, optional): 活动第几天 ,
state (integer, optional): 活动状态 0=未开始 1=活动进行中 2=活动已结束 ,
timeline (string, optional): 时间段 ,
userNumCount (integer, optional): 当天用户未使用的号码数量
}UserNumGroupInfoRsp {
id (integer, optional): 号码组记录ID ,
numGroup (Array[integer], optional): 号码组合
}





/ops/api/lotto/add-num-group    新增号码组
{
  "code": 0,
  "data": {
    "buyTime": 0,
    "gameProgress": {
      "gameBetting": 0,
      "progressList": [
        {
          "amount": 0,
          "awards": 0,
          "propType": 0,
          "sort": 0,
          "status": 0
        }
      ]
    },
    "taskProgress": [
      {
        "finishNum": 0,
        "idx": 0,
        "state": 0,
        "totalNum": 0
      }
    ]
  },
  "message": "string"
}
ResponseBean«LottoUserProgressRsp» {
code (integer, optional),
data (LottoUserProgressRsp, optional): 响应数据体 ,
message (string, optional): 错误信息
}LottoUserProgressRsp {
buyTime (integer, optional): 购买礼包次数 ,
gameProgress (LottoGameInfoRsp, optional): 游戏进度 ,
taskProgress (Array[SupremeActivityTaskProgressRsp], optional): 任务进度 idx=1每日任务 idx=2充值任务
}LottoGameInfoRsp {
gameBetting (integer, optional): 游戏累消 ,
progressList (Array[LottoGameProgressRsp], optional): 累消进度
}SupremeActivityTaskProgressRsp {
finishNum (integer, optional): 完成数 ,
idx (integer, optional): 位置 1=每日任务 2=充值任务 ,
state (integer, optional): 状态 0=未完成 1=已完成未领取 2=已完成已领取 ,
totalNum (integer, optional): 总数
}LottoGameProgressRsp {
amount (integer, optional): 流水 ,
awards (integer, optional): 奖励数 ,
propType (integer, optional): 类型 1=药丸 2=酒精 3=疫苗 ,
sort (integer, optional): 顺序 ,
status (integer, optional): 状态 0=未完成 1=待领取 2=已领取
}




